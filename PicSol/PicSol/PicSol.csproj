    <Product>PicSol</Product>
    <Authors>Michael Stum &lt;opensource@stum.de&gt;</Authors>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
